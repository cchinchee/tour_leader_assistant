<tr class="tour-list-details row" id="delete_<%= date_available.id %>">
	<td class="tour-list-details col-lg-3"><%= date_available.start_date.strftime('%d %B %Y') %></td>
	<td class="tour-list-details col-lg-3"><%= date_available.end_date.strftime('%d %B %Y') %></td>
	<td class="tour-list-details col-lg-3"><%= link_to "Delete", user_date_available_path(date_available.user_id, date_available.id), method: :delete, data: {confirm: "Do you want to delete this record?"}, remote:true %></td>

</tr>